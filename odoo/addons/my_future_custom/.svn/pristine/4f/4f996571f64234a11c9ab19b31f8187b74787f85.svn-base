<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="view_my_future_search_paket_form" model="ir.ui.view">
            <field name="name">my.future.search.paket.views</field>
            <field name="model">myfuture.search.paket</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="Check Out Paket">
                    <p>Total Scan: <field name="count_scanner" readonly="1" force_save="1"/></p>
	                <field name="paket"/>
                    <footer>
                    <button name="action_checkout_paket" string="Check Out" type="object"
                        class="btn-primary" />
                        <button string="Close" class="oe_link" special="cancel" />
                    </footer>
                </form>
            </field>
        </record>

        <record id="action_my_future_search_paket" model="ir.actions.act_window">
            <field name="name">Check Out Paket</field>
            <field name="res_model">myfuture.search.paket</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
            <field name="view_id" ref="view_my_future_search_paket_form"/>
        </record>
        
    </data>
</odoo>
