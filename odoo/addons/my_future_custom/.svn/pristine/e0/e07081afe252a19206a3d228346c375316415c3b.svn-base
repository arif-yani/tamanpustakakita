<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<record id="view_partner_form_inherit_my_future" model="ir.ui.view">
            <field name="name">view.partner.form.inherit.my.future</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="base.view_partner_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='category_id']" position="after">
                    <field name="is_blacklist_customer" widget="boolean_toggle"/>
                    <field name="is_announced"/>
                    <field name="invoice_link_download" widget="url"/>
                    <field name="payment_link" widget="url"/>
                </xpath>
            </field>
    </record>
</odoo>