<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">

    <t t-name="sgeede_rebrand.SessionExpireDialog" t-inherit="web.SessionExpiredDialog" t-inherit-mode="extension">
      <xpath expr="//div[@role='alert']" position="replace">
        <div role="alert">
          <p style="white-space: pre-wrap;">
            Your SGEEDE ERP session expired. The current page is about to be refreshed.
          </p>
        </div>
      </xpath>
    </t>

</templates>